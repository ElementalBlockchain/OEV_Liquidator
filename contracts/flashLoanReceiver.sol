    }
    // TODO maybe make params an array here and unpack them in a for loop and send to myFlashLoanCall
    function doLiquidation(address[] calldata api3_server, bytes[] calldata encoded_data, uint256[] calldata values, address[] memory tokens, uint256[] memory amounts, uint256[] memory modes, bytes memory params) external onlyOwner() payable {
        updatePrice(api3_server, encoded_data, values);
        myFlashLoanCall(tokens, amounts, modes, params);
    }

    function myFlashLoanCall(address[] memory tokens, uint256[] memory amounts, uint256[] memory modes, bytes memory params) public {
        address receiverAddress = address(this);
        address[] memory assets = new address[](tokens.length);
        for (uint i = 0; i < assets.length; i++) {
          assets[i] = address(tokens[i]);
        }
        address onBehalfOf = address(this);
        uint16 referralCode = 0;
        ILendingPool lendingPool = ILendingPool(lending_pool_address);
        lendingPool.flashLoan(receiverAddress, assets, amounts, modes, onBehalfOf, params, referralCode);
    }

    // TODO remove test user statements
    function executeOperation(address[] calldata assets, uint256[] calldata amounts, uint256[] calldata premiums, address initiator, bytes calldata params) external onlyLending() returns (bool) {
      for (uint i = 0; i < assets.length; i++) {
        (address collateral, address user) = paramsToAddresses(params); // params is a 40 byte concatenation of the collateral token and the target user
        if (user == 0x0000000000000000000000000000000000000069) { // 0x69 is a test user, so for testing do a swap to get collateral instead of a liquidation call
          if (swap_version == 2) {
          swapERC20V2(assets[i], collateral, premiums[i], 0, swap_router);  // get some tokens to mock a post-liquidation balance for testing
          } else {
            swapERC20V3(assets[i], collateral, premiums[i], 0, swap_router);   // get some tokens to mock a post-liquidation balance for testing
          }
        } else {
          liquidationCall(lending_pool_address, collateral, assets[i], user, MAXINT); // reserve (assets[i]) is used to repay the loan and now we have the collateral.
        }
        uint256 collateral_balance = IERC20(collateral).balanceOf(address(this)); // collateral balance is retrieved
        if (swap_version == 2) {
          swapERC20V2(collateral, assets[i], collateral_balance, 0, swap_router); // collateral is swapped for the reserve
        } else {
          swapERC20V3(collateral, assets[i], collateral_balance, 0, swap_router); // collateral is swapped for the reserve
        }
      }
      for (uint i = 0; i < assets.length; i++) {
          uint amountOwing = amounts[i].add(premiums[i]); // the flash loan fee
          IERC20(assets[i]).approve(address(lending_pool_address), amountOwing); // approve lending contract to withdraw what it's owed
      }
      return true;
    }

    function swapERC20V2(address tokenIn, address tokenOut, uint amountIn, uint amountOutMin, address router) public onlyLending() returns(uint[] memory amounts) {
        address[] memory path = new address[](2);
        path[0] = tokenIn;
        path[1] = tokenOut;
        IERC20 token = IERC20(path[0]);
        uint amount;
        if (amountIn == MAXINT) {
            amount = token.balanceOf(address(this));
            token.approve(address(router), amount);
        } else {
            amount = amountIn;
        }

        token.approve(address(router), amountIn);
        require((token.balanceOf(address(this)) >= amount), "your balance is too low");
        IUniswapV2Router swap_v2 = IUniswapV2Router(router);
        return swap_v2.swapExactTokensForTokens(amount, amountOutMin, path, address(this), block.timestamp+300);
    }

    function swapERC20V3(address tokenIn, address tokenOut, uint amountIn, uint amountOutMin, address router) public onlyLending() returns(uint256 result){
        IERC20 token =  IERC20(tokenIn);
        if (amountIn == MAXINT) {
            amountIn = token.balanceOf(address(this));
        }
        require((token.balanceOf(address(this)) >= amountIn), "your balance is too low");
        token.approve(address(router), amountIn);
        IUniswapV3Router swaprouter = IUniswapV3Router(router);
        IUniswapV3Router.ExactInputParams memory params = ISwapRouter.ExactInputParams(
            abi.encodePacked(tokenIn, uint24(3000), tokenOut), address(this), block.timestamp+300, amountIn, amountOutMin
        );
        result = swaprouter.exactInput(params);
        return result;
    }

    function liquidationCall(address pool, address collateral, address reserve, address user, uint256 amount) public {
        ILendingPool lendingPool = ILendingPool(pool);
        (,,,,,uint256 hf) = lendingPool.getUserAccountData(user);
        require((hf < 1000000000000000000), "HF>1");
        IERC20 token = IERC20(reserve);
        require((token.balanceOf(address(this)) > 0), "your reserve balance is 0");
        token.approve(address(pool), amount);
        lendingPool.liquidationCall(collateral, reserve, user, amount, false);
    }

    function updatePrice(address[] calldata api3_server, bytes[] calldata data, uint256[] calldata values) public payable {
        externalMulticallWithValue(api3_server, data, values);
    }

    function withdrawERC20(address tokenAddress) external onlyOwner {
        IERC20 token = IERC20(tokenAddress);
        token.transfer(owner(), token.balanceOf(address(this)));
    }

    function withdraw(uint amount) public onlyOwner {
          payable(owner()).transfer(amount);
    }

    // params is a 40 byte bytestring composed of the collateral token and the target user
    function paramsToAddresses(bytes memory params) private pure returns (address collateral, address user) {
      assembly {
        collateral := mload(add(params,20))
        user := mload(add(params,40))
      }
    }

    function updateLendingPool(address _lending_pool_address) external onlyOwner() {
      lending_pool_address = _lending_pool_address;
    }

    function updateRouter(address _swap_router) external onlyOwner() {
      swap_router =_swap_router;
    }

}
